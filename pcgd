//Hair Dynamic, with potentially many hairs, 
//they are blue and linked 2-2 when "upside"
// and 		red and linked 3-3 when "downside"

//BLUE : 6
//WHITE : 19
//RED : 10
//GREEN : 2

dynamic:
	//1
	cycle:
		parts:
			subgraph:
				v:	B(6)
				v:	W(19)
				v:	G(2)
				
				e:	W:2,B:2
				e:	W:1,G:0
				
				s:	W:0
				s:	G:1
			:endsubgraph
			subgraph:
				v:	B(6)
				v:	W(19)
				v:	G(2)
				
				e:	G:2,B:2
				e:	W:1,G:0
				
				s:	W:0
				s:	G:1
			:endsubgraph
		:endparts
		attachments:
			map:
				(W:0)(W:0)
				(G:1)(G:1)
			:endmap
			map:
				(W:0)(W:0)
				(G:1)(G:1)
			:endmap
		:endattachments
	:endcycle
